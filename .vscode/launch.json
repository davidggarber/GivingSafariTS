  "version": "0.2.0",
  "configurations": [
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch w/o compiling",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\Docs\\context.html",
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Browser",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\23\\CropDusters.html",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Docs",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\Docs\\word.html",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch DGG",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\DGG\\trucker.html",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch GS 23",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\23\\indexx.html",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch GS 24",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\24\\menuu.html",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Today's puzzle",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "url": "${workspaceFolder}\\Docs\\formulas.html",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "name": "Python: buildKit.py",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/buildKit.py",
      "cwd": "${workspaceFolder}"
    }
  ]
}